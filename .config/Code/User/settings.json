    "diffEditor.ignoreTrimWhitespace": false,
    "editor.accessibilitySupport": "off",
    "editor.bracketPairColorization.enabled": true,
    "editor.detectIndentation": false,
    "editor.fontFamily": "'JetBrains Mono', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontLigatures": true,
    "editor.fontSize": 16,
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.formatOnSaveMode": "modificationsIfAvailable",
    "editor.formatOnType": true,
    "editor.inlineSuggest.enabled": true,
    "editor.lineNumbers": "relative",
    "editor.linkedEditing": true,
    "editor.minimap.enabled": false,
    "editor.minimap.renderCharacters": false,
    "editor.minimap.showSlider": "always",
    "editor.minimap.size": "fill",
    "editor.renderWhitespace": "all",
    "editor.wordWrap": "bounded",
    "files.associations": {
        "*.css": "tailwindcss"
    },
    "files.exclude": {
        "**/**.js": {
            "when": "$(basename).tsx"
        },
        "**/*.js": {
            "when": "$(basename).ts"
        }
    },
    "files.insertFinalNewline": true,
    "files.trimFinalNewlines": true,
    "files.trimTrailingWhitespace": true,
    "files.watcherExclude": {
        "**/.svelte-kit/**": true,
        "**/previous-iterations/**": true,
        "**/target/**": true
    },
    "github.copilot.enable": {
        "*": true,
        "markdown": true,
        "plaintext": false,
        "yaml": false
    },
    "go.toolsManagement.autoUpdate": true,
    "html.format.indentHandlebars": true,
    "html.format.indentInnerHtml": true,
    "html.format.templating": true,
    "julia.enableCrashReporter": true,
    "julia.enableTelemetry": true,
    "julia.NumThreads": 6,
    "julia.symbolCacheDownload": true,
    "jupyter.askForKernelRestart": false,
    "jupyter.interactiveWindowMode": "perFile",
    "jupyter.notebookFileRoot": "${workspaceFolder}",
    "latex-workshop.bibtex-fields.sort.enabled": true,
    "latex-workshop.bibtex-format.sort.enabled": true,
    "latex-workshop.latex.autoBuild.run": "never",
    "latex-workshop.latex.autoClean.run": "onFailed",
    "latex-workshop.latex.tools": [
        {
            "args": [
                "-xelatex",
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "%DOC%"
            ],
            "command": "latexmk",
            "name": "latexmk"
        }
    ],
    "latex-workshop.linting.chktex.enabled": true,
    "latex-workshop.linting.delay": 1000,
    "latex-workshop.texcount.autorun": "onSave",
    "latex-workshop.view.pdf.viewer": "tab",
    "liveServer.settings.donotShowInfoMsg": true,
    "ltex.additionalRules.enablePickyRules": true,
    "ltex.additionalRules.motherTongue": "pt-BR",
    "ltex.completionEnabled": true,
    "markdown.preview.typographer": true,
    "notebook.lineNumbers": "on",
    "polacode.transparentBackground": true,
    "prettier.jsxBracketSameLine": true,
    "prettier.proseWrap": "always",
    "prettier.tabWidth": 4,
    "python.analysis.typeCheckingMode": "strict",
    "python.formatting.provider": "black",
    "python.linting.banditEnabled": true,
    "python.linting.flake8Enabled": true,
    "python.linting.mypyEnabled": true,
    "python.linting.pydocstyleEnabled": true,
    "python.showStartPage": false,
    "python.testing.pytestEnabled": true,
    "quarto.mathjax.theme": "dark",
    "quarto.render.previewType": "external",
    "quarto.render.renderOnSave": true,
    "rust-analyzer.assist.importEnforceGranularity": true,
    "rust-analyzer.cargo.allFeatures": true,
    "rust-analyzer.debug.openDebugPane": true,
    "rust-analyzer.experimental.procAttrMacros": true,
    "rust-analyzer.files.excludeDirs": [
        "**/previous-iterations/**"
    ],
    "rust-analyzer.hoverActions.references": true,
    "rust-analyzer.lens.methodReferences": true,
    "rust-analyzer.lens.references": true,
    "rust-analyzer.rustfmt.enableRangeFormatting": true,
    "rust.clippy_preference": "on",
    "svelte.enable-ts-plugin": true,
    "svelte.plugin.svelte.defaultScriptLanguage": "ts",
    "svelte.plugin.svelte.format.config.singleQuote": true,
    "svelte.plugin.svelte.format.config.svelteBracketNewLine": false,
    "svelte.plugin.svelte.format.config.svelteStrictMode": true,
    "svelte.plugin.svelte.note-new-transformation": false,
    "svelte.plugin.svelte.useNewTransformation": true,
    "tailwindCSS.emmetCompletions": true,
    "tailwindCSS.includeLanguages": {
        "svelte": "html"
    },
    "terminal.integrated.commandsToSkipShell": [
        "language-julia.interrupt"
    ],
    "typescript.updateImportsOnFileMove.enabled": "always",
    "window.zoomLevel": 0,
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook"
    }
}
